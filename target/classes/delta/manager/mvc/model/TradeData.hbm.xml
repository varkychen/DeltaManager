<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="delta.manager.mvc.model.TradeData" table="TRADEDATA">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="counterparty" column="COUNTERPARTY" type="string"/>
		<property name="notional" column="NOTIONAL" type="double" />
		<property name="underlying" column="UNDERLYING" type="string" />
	</class>
</hibernate-mapping>